 <div class="home-v3-ads-block animate-in-view fadeIn animated" data-animation=" animated fadeIn">
     <div class="ads-block row">
         <% for( let i=0 ; i< productBaner.length ; i++){ %>

         <div class="ad col-xs-12 col-sm-6">
             <div class="media">
                 <div class="media-left media-middle">
                     <img src="<%= productBaner[i].src %> " alt="">
                 </div>
                 <div class="media-body media-middle">
                     <div class="ad-text">
                         <%= productBaner[i].name %> <strong>Deals</strong> in
                         <%= productBaner[i].category %>
                     </div>
                     <div class="ad-action">
                         <a href="#">Shop now</a>
                     </div>
                 </div>
             </div>
         </div>

         <% } %>
     </div>
 </div><!-- /.home-v3-ads-block -->